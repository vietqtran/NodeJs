<!DOCTYPE html>
<html lang="en">

<head>
      <meta charset="UTF-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <link rel="stylesheet" href="/css/style.css">
      <title>NodeJs Basic</title>
</head>

<body>
      <div class="topnav">
            <a class="active" href="#home">Home</a>
            <a href="#news">News</a>
            <a href="#contact">Contact</a>
            <a href="#about">About</a>
      </div>

      <div style="padding-left:16px">
            <h2>List users</h2>
            <table>
                  <tr>
                        <th>ID</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Manage</th>
                  </tr>
                  <%= test%>
                        <% for(let i=0; i < dataUser.length; i++) { %>
                              <tr>
                                    <td>
                                          <%= dataUser[i].id %>
                                    </td>
                                    <td>
                                          <%= dataUser[i].firstName %>
                                    </td>
                                    <td>
                                          <%= dataUser[i].lastName %>
                                    </td>
                                    <td>
                                          <button><a target="blank"
                                                      href="/details/user/<%= dataUser[i].id %>">Details</a></button>
                                          <button>Edit</button>
                                          <button>Delete</button>
                                    </td>
                              </tr>
                              <% } %>
            </table>
      </div>
</body>

</html>